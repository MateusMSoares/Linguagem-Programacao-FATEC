<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml" fx:controller="com.example.dadosmeteorologicos.PrincipalController">
    <children>
        <Label layoutX="104.0" layoutY="90.0" text="Salve seu nome e a senha no banco :)">
            <font>
                <Font name="Yu Gothic Regular" size="24.0" />
            </font>
        </Label>
        <TextField fx:id="nome" layoutX="300.0" layoutY="150.0" promptText="Insira o Nome" />
        <Label layoutX="160.0" layoutY="145.0" text="Nome:">
            <font>
                <Font size="24.0" />
            </font>
        </Label>
        <Label layoutX="137.0" layoutY="183.0" text="Senha Secreta:">
            <font>
                <Font size="24.0" />
            </font>
        </Label>
        <PasswordField fx:id="senha" layoutX="300.0" layoutY="188.0" promptText="Insira a senha" />
        <Button fx:id="submitButton" layoutX="261.0" layoutY="256.0" mnemonicParsing="false" onAction="#PegaDados" prefHeight="35.0" prefWidth="78.0" text="Salvar" />
    </children>
    <cursor>
        <Cursor fx:constant="DEFAULT" />
    </cursor>
</AnchorPane>